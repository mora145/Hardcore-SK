<?xml version="1.0" encoding="utf-8" ?>
<Defs>

 
   <RecipeDef Abstract="True" Name="RecipeBasicProsthesis">
      <workSpeedStat>TailoringSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <workAmount>2500</workAmount>
    	<workSkillLearnFactor>0.8</workSkillLearnFactor>
   </RecipeDef>
   
   <RecipeDef ParentName="RecipeBasicProsthesis">
      <defName>CreatePegLeg</defName>
      <label>Craft Peg Leg</label>
      <description>Crafts a simple prosthetic peg leg. Produces 1.</description>
      <jobString>Crafting peg leg.</jobString>
      <soundWorking>Recipe_MakeWoodPlanks_Hand</soundWorking>
      <products>
         <PegLeg>1</PegLeg>
      </products>
      <ingredients>
         <li>
            <filter>
					<categories>
						<li>Metallic</li>
                    </categories>
            </filter>
            <count>10</count>
         </li>
          <li>
            <filter>
               <categories>
                  <li>Woody</li>
               </categories>
            </filter>
            <count>25</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>ComponentIndustrial</li>
               </thingDefs>
            </filter>
            <count>2</count>
         </li>
         <li>
            <filter>
               <thingDefs>
                  <li>Mechanism</li>
               </thingDefs>
            </filter>
            <count>1</count>
         </li>
      </ingredients>
      <fixedIngredientFilter>
					<categories>
						<li>SLDBar</li>
						<li>USLDBar</li>
						<li>Woody</li>
                    </categories>
         <thingDefs>
            <li>ComponentIndustrial</li>
            <li>Mechanism</li>
         </thingDefs>
      </fixedIngredientFilter>
		<skillRequirements>
			<Crafting>5</Crafting>
		</skillRequirements>
      <workSkill>Crafting</workSkill>
   </RecipeDef>
   
   
