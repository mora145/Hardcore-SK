<Defs>
  <!-- TM_Plants.xml -->
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Pomanasyrrium</defName>
    <label>extract pomanasyrrium seeds</label>
    <description>Extract seeds from Pomanasyrrium.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>TM_RawPomanasyrrium</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>TM_RawPomanasyrrium</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Pomanasyrrium>3</Seed_Pomanasyrrium>
    </products>
    <researchPrerequisite>TM_PlantCultivation</researchPrerequisite>
  </RecipeDef>
  <RecipeDef ParentName="ExtractSeed">
    <defName>ExtractSeed_Paracyte</defName>
    <label>extract paracyte seeds</label>
    <description>Extract seeds from Magicyte.</description>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>RawMagicyte</li>
          </thingDefs>
        </filter>
        <count>4</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>RawMagicyte</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Seed_Paracyte>3</Seed_Paracyte>
    </products>
    <researchPrerequisite>TM_PlantCultivation</researchPrerequisite>
  </RecipeDef>
</Defs>
